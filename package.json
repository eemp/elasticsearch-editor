{
  "name": "elasticsearch-scribe",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch-js": "watchify -t [ babelify --presets [ react es2015 ] ] _site/src/index.js  -o _site/build/bundle.js",
    "watch-css": "node-sass --watch _site/sass --output _site/css",
    "watch": "npm run watch-css && npm run watch-js",
    "build-js": "browserify -t [ babelify --presets [ react es2015 ] ] _site/src/index.js  -o _site/build/bundle.js",
    "build-css": "node-sass _site/sass --output _site/css",
    "build": "npm run build-css && npm run build-js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "elasticsearch",
    "scribe",
    "record",
    "document",
    "editor"
  ],
  "author": "eemp",
  "license": "Apache2",
  "dependencies": {
    "brace": "^0.7.0",
    "elasticsearch": "^10.0.1",
    "react": "^0.14.3",
    "react-ace": "^2.8.0",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.5",
    "react-tabs": "^0.5.1",
    "redux": "^3.0.5",
    "redux-thunk": "^1.0.3"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "bower": "^1.7.0",
    "express": "^4.13.3",
    "node-sass": "^3.4.2"
  }
}
